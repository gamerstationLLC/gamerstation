{
  "name": "gamerstation",
  "private": true,
  "scripts": {
    "lol:overrides": "node scripts/gen_spells_overrides.mjs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lol:fetch": "node scripts/fetch-lol-ddragon.mjs",
    "lol:items": "node scripts/fetch-lol-items.mjs",
    "lol:update": "npm run lol:fetch && npm run lol:items",
    "bloxfruits:items": "node scripts/bloxfruits/build-items.mjs",
    "lol:update:data": "node scripts/fetch-lol-ddragon.mjs && node scripts/fetch-lol-items.mjs",
    "lol:update:overrides": "node scripts/gen_spells_overrides.mjs",
    "lol:update:meta": "tsx scripts/riot/build-meta-builds.ts",
    "lol:finalize:meta": "tsx scripts/riot/finalize-meta-builds-from-cache.ts",
    "lol:update:all": "npm run lol:update:data && npm run lol:update:overrides && npm run lol:update:meta && (npm run lol:finalize:meta || true)",
    "lol:leaderboards": "tsx scripts/riot/build-leaderboards.ts",
    "lol:finalize:leaderboards": "tsx scripts/riot/finalize-leaderboards.ts",
    "lol:all:leaderboards": "tsx scripts/riot/build-leaderboards.ts && tsx scripts/riot/finalize-leaderboards.ts",
    "lol:champion-tiers": "npx tsx ./scripts/riot/build-champion-tiers.ts",
    "lol:finalize:champion-tiers": "npx tsx ./scripts/riot/finalize-champion-tiers-from-cache.ts"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "dotenv": "^17.2.3",
    "fflate": "^0.8.2",
    "next": "16.1.1",
    "node-fetch": "^3.3.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cheerio": "^1.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
